/*20191023 Homework */

#include <stdio.h>

int main()
{
    int i,j;

    for (i=1;i<=9;i++){                      
        for (j=1;j<=9;j++)
            printf("%dx%d=%d\t", i,j,i*j);    /* \t空格 */
        printf("\n");                         /* \n換行 */
    }
    
    return 0;
}


/* NOTE */

/* for (起始值,判斷條件,更新值) */
/* for(i=1;i<=9;i++) */

